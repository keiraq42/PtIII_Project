@article{stoffler_glasses_1984,
	title = {Glasses formed by hypervelocity impact},
	volume = {67},
	issn = {0022-3093},
	url = {https://www.sciencedirect.com/science/article/pii/0022309384901716},
	doi = {10.1016/0022-3093(84)90171-6},
	abstract = {Impact glasses occur as mineral glasses and rock glasses along with shock metamorphosed rocks of meteorite craters. Rock glasses may be 1.(1) melt ejecta,2.(2) parts of a coherent melt layer inside the crater cavity, or3.(3) dikes or veins. They contain mineral and lithic clasts of varied degrees of shock, and display flow structure and vesiculation. The composition of rock glasses at a particular crater can be matched by that of the target, which may be a mixture of different basement rocks. Mineral glasses are constituents of shocked rocks found as clasts within polymict breccias or in the top section of the crater basement. Among mineral glasses, “diaplectic” glasses can be distinguished from “normal” glasses by their higher density and refractivity, and by differences in texture, structural state, and annealing behavior. Only minerals with “open” structures such as quartz and feldspars undergo the shock-induced transition to diaplectic glass. Diaplectic glass is considered to originate from a dense, “liquid” high pressure phase quenched to glass before the shock pressure is released. Normal glass is quenched from a melt which is still liquid after pressure release. Mineral glasses are formed by shock compression of silica or silicate minerals (single crystals or non-porous rocks) in the pressure range of about 25 to 55 GPa. Rock glasses (i.e. total rock melts) require peak pressures in excess of 60–80 GPa. In porous rocks and sediments, interstitial glass formation takes place at pressures as low as about 5 GPa.},
	number = {1},
	urldate = {2024-03-13},
	journal = {Journal of Non-Crystalline Solids},
	author = {Stöffler, D.},
	month = sep,
	year = {1984},
	pages = {465--502},
}

